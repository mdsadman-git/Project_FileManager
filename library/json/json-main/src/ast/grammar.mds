Expression
  : ObjectExpression
  : ArrayExpression
  ;

ObjectExpression
  : '{' (ObjectLiteral) * '}'
  ;

ObjectLiteral: 
  : KeyLiteral ':' ValueLiteral ','?
  ;

KeyLiteral
  : StringLiteral 
  ;

ValueLiteral
  : ObjectExpression
  : ArrayExpression
  : StringLiteral 
  : Numeric
  : Boolean
  : Null
  ;

StringLiteral
  : '"' [.*]* '"'
  ;

Numeric
  : [0-9\.]+
  ;

Boolean
  : true
  : false
  ;

Null
  : null
  ;

ArrayExpression
  : '[' (ValueLiteral','?)* ']'
  ;